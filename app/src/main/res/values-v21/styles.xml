<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
        Base application theme for API 21+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-21/styles.xml on API 21+ devices.
    -->
    <style name="AmniXTheme" parent="android:Theme.DeviceDefault.Light">
        <!-- API 21 theme customizations can go here. -->
        <item name="android:statusBarColor">@android:color/holo_blue_light</item>
        <item name="android:colorAccent">@android:color/holo_blue_light</item>
        <item name="android:colorPrimary">@android:color/holo_blue_light</item>
    </style>
</resources>